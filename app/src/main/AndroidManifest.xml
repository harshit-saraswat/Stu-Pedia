<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.codezero.stu_pedia">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:hardwareAccelerated="false"
        android:icon="@drawable/stupedialogo"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@drawable/stupedialogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".Dashboard" />
        <activity android:name=".MainView" />
        <activity android:name=".Answer" />
        <activity android:name=".Question" />
        <activity android:name=".AboutUs" />
        <activity android:name=".ContactUs" />
        <activity android:name=".CheckEvents" />
        <activity android:name=".eventcheck" />
        <activity android:name=".PostEvent"></activity>
    </application>

</manifest>